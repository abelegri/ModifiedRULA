    <string name="neckPosturePrompt">Determine the posture of the neck:</string>

    <string name="neckRadioNeutral">0° - 10°</string>
    <string name="neckRadioLight">10° - 20°</string>
    <string name="neckRadioModerate">more than 20°</string>
    <string name="neckRadioExtension">in extension</string>

