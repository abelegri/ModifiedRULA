    <string name="left_button">Left Side</string>
    <string name="both_button">Both Sides</string>

    <!-- ACTIVITY RIGHT/LEFT UPPER ARM POSTURE (UAP) -->
    <string name="rightUAP_prompt">Locate the RIGHT UPPER arm position:</string>
    <string name="leftUAP_prompt">Locate the LEFT UPPER arm position:</string>

    <string name="UpperArmRadioNeutral">-20° to 20°</string>
    <string name="UpperArmRadioExtension">in extension past -20°</string>
    <string name="UpperArmRadioLight">20° to 45°</string>
    <string name="UpperArmRadioModerate">45° to 90°</string>
    <string name="UpperArmRadioExtreme">over 90°</string>

    <!-- ACTIVITY RIGHT/LEFT UPPER ARM ADJUSTMENT (UAA) -->
    <string name="rightUAA_prompt">Select all that apply to the RIGHT arm:</string>
    <string name="leftUAA_prompt">Select all that apply to the LEFT arm:</string>

    <string name="UpperArmCheckShoulder">The shoulder is raised</string>
    <string name="UpperArmCheckAbducted">The upper arm is abducted (away from body)</string>
    <string name="UpperArmCheckSupport">The weight of the arm is supported</string>

    <!-- ACTIVITY RIGHT/LEFT LOWER ARM POSTURE (LAP)-->
    <string name="rightLAP_prompt">Locate the RIGHT LOWER arm position:</string>
    <string name="leftLAP_prompt">Locate the LEFT LOWER arm position:</string>

    <string name="LowerArmRadioNeutral">60° to 100°</string>
    <string name="LowerArmRadioExtension">less than 60°</string>
    <string name="LowerArmRadioFlexion">greater than 100°</string>

    <!-- ACTIVITY RIGHT/LEFT LOWER ARM ADJUSTMENT (LAA) -->
    <string name="rightLAA_prompt">Select all that apply to the RIGHT arm:</string>
    <string name="leftLAA_prompt">Select all that apply to the LEFT arm:</string>

    <string name="LowerArmCheckMidline">The arm crosses the midline of the body</string>
    <string name="LowerArmCheckOutToSide">The arm is working out to the side of the body</string>



    <!-- ACTIVITY RIGHT/LEFT WRIST POSITION (WP) -->
    <string name="rightWP_prompt">Locate the RIGHT wrist position:</string>
    <string name="leftWP_prompt">Locate the LEFT wrist position:</string>

    <string name="WristRadioNeutral">0°</string>
    <string name="WristRadioModerate">-15° to 15°</string>
    <string name="WristRadioExtreme">less than -15° or greater than 15°</string>

    <!-- ACTIVITY RIGHT/LEFT WRIST ADJUSTMENT (WA) -->
    <string name="rightWA_prompt">Select all that apply to the RIGHT wrist:</string>
    <string name="leftWA_prompt">Select all that apply to the LEFT wrist:</string>

    <string name="WristCheckMidline">The wrist is bent from the midline</string>

    <!-- ACTIVITY RIGHT/LEFT WRIST TWIST (WT) -->
    <string name="rightWT_prompt">Determine the RIGHT wrist twist:</string>
    <string name="leftWT_prompt">Determine the LEFT wrist twist:</string>

    <string name="WristTwistRadioMidRange">Twisting in mid-range</string>
    <string name="WristTwistRadioEndRange">Twisting at or near end of range</string>

    <!-- ACTIVITY RIGHT/LEFT/GENERAL MUSCLE USE (MU) -->
    <string name="rightMU_prompt">Is the right arm posture either mainly static (held for > 10 minutes) or repeated > 4X per minute?</string>
    <string name="leftMU_prompt">Is the left arm posture mainly static (held for > 10 minutes) or repeated > 4X per minute?</string>
    <string name="generalMU_prompt">Is the neck, trunk, and leg posture mainly static (held for > 10 minutes) or repeated > 4X per minute?</string>

    <string name="MuscleRadioYes">Yes</string>
    <string name="MuscleRadioNo">No</string>

    <!-- ACTIVITY RIGHT/LEFT/GENERAL FORCE (F) -->
    <string name="rightF_prompt">Determine the force exerted by the right arm:</string>
    <string name="leftF_prompt">Determine the force exerted by the left arm:</string>
    <string name="generalF_prompt">Determine the force exerted by the neck, trunk, and legs:</string>

    <string name="ForceRadioLow">less than 4.4 lbs (intermittent)</string>
    <string name="ForceRadioModerateIntermittent">4.4 to 22 lbs (intermittent)</string>
    <string name="ForceRadioModerateStatic">4.4 to 22 lbs (static or repeated)</string>
    <string name="ForceRadioExtreme">more than 22 lbs or repeated or shocks</string>

